{
  "name": "fitapp",
  "version": "1.0.0",
  "description": "Web Application for Fitness",
  "main": "server.js",
  "scripts": {
    "client-install": "npm install --prefix front-end",
    "start": "node server.js",
    "server": "nodemon server.js",
    "client": "npm start --prefix front-end",
